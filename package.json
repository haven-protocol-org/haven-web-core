{
  "name": "haven-wallet-core",
  "description": "A JavaScript library for using Haven",
  "version": "1.4.44",
  "license": "MIT",
  "repository": "https://github.com/haven-protocol-org/haven-web-core",
  "private": false,
  "main": "index.js",
  "scripts": {
    "start": "todo",
    "build_bundle": "webpack --config ./webpack.bundle.js --display-modules",
    "build_worker": "npm run build_web_worker && npm run build_worker_thread",
    "build_web_worker": "webpack --config ./webpack.web-worker.js",
    "build_worker_thread": "webpack --config ./webpack.worker-thread.js",
    "build_web_tests": "webpack --config ./webpack.tests.js",
    "test": "node --experimental-wasm-threads --experimental-wasm-bulk-memory node_modules/mocha/bin/_mocha 'src/test/TestAll' --timeout 900000000 --exit",
    "debug-test": "node --experimental-wasm-threads --inspect-brk --experimental-wasm-bulk-memory node_modules/mocha/bin/_mocha 'src/test/TestAll' --timeout 900000000 --exit",
    "jsdoc": "jsdoc -r -c ./configs/jsdoc_config.js -d ./docs"
  },
  "engines": {
    "node": "7.4.0"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "async": "2.6.1",
    "crypto-js": "^4.0.0",
    "html5-fs": "0.1.1",
    "memfs": "^3.2.2",
    "mocha": "^7.2.0",
    "net": "^1.0.2",
    "promise-throttle": "^1.1.2",
    "request": "2.88.0",
    "request-promise": "^4.2.6",
    "text-encoding": "^0.7.0",
    "tls": "0.0.1",
    "uuid": "3.3.2",
    "web-worker": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.2",
    "eslint": "5.6.0",
    "jsdoc": "^3.6.6",
    "minimist": "^1.2.5",
    "typescript": "^4.2.4",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "4.1.4",
    "worker-loader": "2.0.0",
    "yargs-parser": "^18.1.3"
  }
}
